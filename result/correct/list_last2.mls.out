fix (f : list -> natopt) =
  fun (x:list) ->
    match x with
    | Nil -> None
    | Cons (h,tl) -> (match tl with
                        | Nil -> None
                        | Cons (th,ttl) -> (match ttl with
                                            | Nil -> Some (h,th)
                                            | Cons (tth,tttl) -> f tl))