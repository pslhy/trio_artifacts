fix (f : list -> list) =
  fun (x:list) ->
    match x with
      | Nil -> Nil
      | Cons (_, Nil) -> x
      | Cons (h1, Cons (h2, t)) ->
           (match compare h1 h2 with
              | LT -> Cons (h1, f (Cons (h2, t)))
              | EQ _ -> f Cons (h2, t)
              | GT _ -> Cons (h1, f (Cons (h2, t))))